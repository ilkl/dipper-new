<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yundasys.beidou.dipper.mapper.PersonMapper" >
    <select id="getAllUser" resultType="com.yundasys.beidou.dipper.model.entity.Person">
        SELECT * FROM t_person
    </select>

    <select id="getOne" resultType="com.yundasys.beidou.dipper.model.entity.Person">
        SELECT * FROM t_person where id =#{id}
    </select>
</mapper>